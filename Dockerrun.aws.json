{
    "AWSEBDockerrunVersion": 2,
    "containerDefinations": [
        {
            "name": "client",
            "image": "parvindran/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "parvindran/multi-server",
            "hostname": "server",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "parvindran/multi-worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "parvindran/multi-nginx",
            "essential": true,
            "portMappings": [
                {
                "hostPort": 80,
                "containerPort": 80
                }
            ],
            "memory": 128,
            "links":["client", "server"]
        }
    ]
}